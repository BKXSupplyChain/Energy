import requests
import json
import time

def tester(s):
    add = 100
    cntJo = 0
    identification = s[:16]
    URL = s[16:]
    while (True):
        payload = {'ID': identification, 'CntOfJoules': cntJo}
        print(payload)
        r = requests.post(URL, data = json.dumps(payload))
        cntJo = cntJo + add
        time.sleep(120)
